<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_app"
        android:supportsRtl="true"
        android:theme="@style/Theme.Knowledge"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.navigationcomponentmodule.NavigationComponentMainActivity" />
        <activity android:name="com.example.flowmodule.compare.FlowCompareActivity" />
        <activity android:name="com.example.graphqlmodule.GraphQLMainActivity" />
        <activity android:name="com.example.errorhandlermodule.ErrorHandlingMainActivity" />
        <activity android:name="com.example.coroutines.CoroutinesActivity" />
        <activity android:name="com.example.coroutinescodelabmodule.main.CoroutinesCodelabActivity" />
        <activity android:name="com.example.flowcodelabmodule.ui.FlowCodelabActivity" />
        <activity android:name="com.example.flowmodule.FlowActivity" />
    </application>

</manifest>